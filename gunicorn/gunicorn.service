[Unit]
Description=gunicorn daemon
Requires=gunicorn.socket
After=network.target
[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/home/ubuntu/crm_clothes
ExecStart=/home/ubuntu/env/bin/gunicorn --access-logfile - --workers 3 --bind unix:/home/ubuntu/crm_clothes/crm_clothes/crm_clothes.sock crm_clothes.wsgi:application

       
[Install]
WantedBy=multi-user.target